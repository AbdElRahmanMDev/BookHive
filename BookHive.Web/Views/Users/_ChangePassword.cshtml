
@model PasswordFormViewModel
<form asp-action="ChangePassword"
      data-ajax-method="post"
      data-ajax="true"
      data-ajax-begin="OnModelBegin"
      data-ajax-success="onModelSuccess"
      data-ajax-failure="showErrorMessage"
      data-ajax-complete="OnModelComplete">

    
    
<input type="hidden" asp-for=Id />

<div class="mb-5">
    <div class="form-floating mb-2">
        <input class="form-control" asp-for="Password" />
        <label asp-for="Password"></label>
    </div>
    <span asp-validation-for="Password" class="text-danger"></span>
</div>



<div class="mb-5">
    <div class="form-floating mb-2">
        <input class="form-control" asp-for="ConfirmPassword" />
        <label asp-for="ConfirmPassword"></label>
    </div>
    <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
</div>

    <partial name="_footerModel" />
</form>